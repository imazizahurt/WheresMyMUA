
@model List<Artist>

<h2> A MUA NEAR YOU</h2>

<table class="table-responsive table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th>Name</th>
            <th>Specialty</th>
            <th>Location</th>
            <th>Phone</th>
        </tr>
    </thead>

    <tbody>
        @foreach (Artist artist in Model)
        {
            <tr>
                <td>@artist.Name</td>
                <td>@artist.Specialty</td>
                <td>@artist.Location</td>
                <td>@artist.Phone</td>
            </tr>
        }
    </tbody>
</table>








