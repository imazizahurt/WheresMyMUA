
@model List<Artist>


<style>
    body {
        background-image: url("/images/mua_work/graphics/circle_makeup.jpg");
        background-size: 100%;
        background-position: 0% 35%;
    }
</style>



<body>
    <div>
        <h2 style="font-family:Pintgram-Italic; justify-content: center; margin:auto; text-align:center"> Makeup Artist Near You</h2>

        <table style="font-family:LittlePea-Demo-Regular; justify-content: center; margin:auto" class="table-responsive table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Specialty</th>
                    <th>Location</th>
                    <th>Phone</th>
                </tr>
            </thead>

            <tbody>
                @foreach (Artist artist in Model)
                {
                    <tr>
                        <td>@artist.Name</td>
                        <td>@artist.Specialty</td>
                        <td>@artist.Location</td>
                        <td>@artist.Phone</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>








